        function Upload() {
            //Get reference of FileUpload.
            var fileUpload = document.getElementById("FileUpload");

            //Check whether the file is valid Image.
            var regex = new RegExp("([a-zA-Z0-9\s_\\.\-:])+(.jpg|.png|.gif)$");
            if (regex.test(fileUpload.value.toLowerCase())) {

                //Check whether HTML5 is supported.
                if (typeof (fileUpload.files) != "undefined") {
                    //Initiate the FileReader object.
                    var reader = new FileReader();
                    //Read the contents of Image File.
                    reader.readAsDataURL(fileUpload.files[0]);
                    reader.onload = function (e) {
                        //Initiate the JavaScript Image object.
                        var image = new Image();

                        //Set the Base64 string return from FileReader as source.
                        image.src = e.target.result;

                        //Validate the File Height and Width.
                        image.onload = function () {

                            var height = this.height;
                            var width = this.width;
                            if (height < 2400 || width < 2400 || height != width) {

                                //alert("Height and Width must not Less than 1400px.");
                                document.getElementById('imageSizeAlert').style.display = 'block';
                                document.getElementById('imageBrowserAlert').style.display = 'none';
                                document.getElementById('imageTypeAlert').style.display = 'none';
                                $('#FileUpload').val(null);
                                return false;
                            }


                            var files = !!fileUpload.files ? fileUpload.files : [];
                            if (!files.length || !window.FileReader) {
                                $("#PreviewPicture").attr("src", "~/assets/img/img-placeholder.jpg");
                                return;
                            }// Check if File is selected, or no FileReader support
                            if (/^image/.test(files[0].type)) { //  Allow only image upload
                                var ReaderObj = new FileReader(); // Create instance of the FileReader
                                ReaderObj.readAsDataURL(files[0]); // read the file uploaded
                                ReaderObj.onloadend = function () { // set uploaded image data as background of div
                                    $("#PreviewPicture").attr("src", this.result);
